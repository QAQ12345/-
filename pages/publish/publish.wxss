page{
  background: #f5f5f5;
}
.form{
  width: 100%;
  font-size: 18px;
  display: flex;
  color: #999;
  line-height: 100rpx;
  border-bottom: 1px solid #ddd;
  background: #fff;
}
.form-title{
  width: 28%;
  padding-left: 20rpx;
}
.form-info{
  height: 100rpx;
  color: #999;
}
.form-send{
  width: 100%;
  margin-top: 30rpx;
  text-align: center;
}
.form-sendMessage{
  width: 95%;
  line-height: 96rpx;
  color: #fff;
  display: inline-block;
  border-radius: 10rpx;
  background: #ff9700;
  text-align: center;
}

.succ{
  color: #22cad3;
  font-weight:600;
}
.succ-send{
  height: 150rpx;
  text-align: center;
}
.succ-icon{
  height:105rpx;
  padding: 30rpx;
  vertical-align: middle;
}
.succ-item{
  line-height: 80rpx;
  text-align: center;
}
.succ-goto{
  text-align: center;
  line-height: 80rpx;
  margin-top: 30rpx;
}
.succ-navigator{
  width: 95%;
  color: #fff;
  text-align: center;
  font-weight:300;
  display: inline-block;
  border-radius:10rpx;
  background: #ff9700;
}